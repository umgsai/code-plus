{
  "name": "code-plus",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "针对点评Code平台的插件",
  "icons": {
    "16": "image/computer.png",
    "48": "image/computer.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_title": "code-plus",
    "default_icon": "image/computer.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["script/jquery-3.1.1.min.js","script/code-plus.js"],
      "matches": [
        "http://*/*",
        "https://*/*"    
    	],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
        "https://*/*"   
  ]
}